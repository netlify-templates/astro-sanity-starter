---
import { marked } from 'marked';
import { type Footer } from 'types';

interface Props extends Footer {}

const { text } = Astro.props;
---

{
    text && (
        <footer class="px-4 py-16 sm:px-5 sm:py-24">
            <div class="mx-auto text-center max-w-7xl" set:html={marked.parse(text)} />
        </footer>
    )
}
